{
    "name": "rogervila/random-app-example",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Roger Vil√†",
            "email": "roger.vila.camon@hp.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Random\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "rogervila/global-namespace": "dev-main"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0 || ^10.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
