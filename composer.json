{
    "name": "rogervila/global-namespace",
    "description": "Prefix global PHP functions under a namespace",
    "keywords": [
        "php global namespace"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Roger Vil√†",
            "email": "rogervila@me.com"
        }
    ],
    "require": {},
    "require-dev": {
        "php": "^7.2|^8.0",
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "PHP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
